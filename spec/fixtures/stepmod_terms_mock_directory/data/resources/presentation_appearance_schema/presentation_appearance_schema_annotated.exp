      direction_counts         : SET [1:2] OF direction_count_select;
    WHERE
      WR1: (HIINDEX(direction_counts) = 1)
                        XOR
         (TYPEOF(direction_counts[1]) <>
          TYPEOF(direction_counts[2]));
  END_ENTITY;

  ENTITY surface_style_reflectance_ambient;
