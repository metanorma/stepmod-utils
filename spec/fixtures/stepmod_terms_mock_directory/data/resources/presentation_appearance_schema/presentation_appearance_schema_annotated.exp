      WR1: SIZEOF( QUERY( c1 <* characteristics |
           SIZEOF( QUERY( c2 <* characteristics - c1 |
           TYPEOF (c1) = TYPEOF (c2)
           )) > 0
           )) = 0;
  END_ENTITY;

  ENTITY text_style_with_justification
